{
	"name": "korean-studio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel keystone:dev next:dev",
		"build": "npm run keystone:build && npm run next:build",
		"postinstall": "keystone postinstall",
		"keystone:dev": "keystone dev",
		"keystone:build": "keystone build --no-ui",
		"keystone:start": "keystone start",
		"next:dev": "next dev --turbo",
		"next:build": "next build",
		"next:start": "next start",
		"lint": "biome lint -- apply <files>",
		"format": "biome format <files> --write",
		"check": "biome check --apply <files>",
		"prepare": "husky",
		"lint-staged": "lint-staged",
		"script:docs-desc": "tsx scripts/generate-doc-desc.ts",
		"script:docs-file": "tsx scripts/generate-doc-file.ts",
		"script:docs-modified": "tsx scripts/generate-doc-last-modified.ts",
		"script:generate-search-index": "tsx scripts/generate-search-index.ts"
	},
	"dependencies": {
		"@google/generative-ai": "^0.21.0",
		"@keystone-6/auth": "^8.0.0",
		"@keystone-6/core": "^6.3.0",
		"@keystone-6/document-renderer": "^1.1.2",
		"@keystone-6/fields-document": "^9.1.0",
		"@mdx-js/loader": "^3.1.0",
		"@mdx-js/react": "^3.1.0",
		"@next/mdx": "15.0.2",
		"@next/third-parties": "15.0.2",
		"@prisma/client": "^5.16.1",
		"@svgr/webpack": "^8.1.0",
		"@types/mdx": "^2.0.13",
		"@vercel/analytics": "^1.3.1",
		"@vercel/speed-insights": "^1.0.12",
		"ahooks": "^3.8.0",
		"canvas-confetti": "^1.9.3",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"es-hangul": "^2.1.0",
		"flexsearch": "^0.7.43",
		"github-markdown-css": "^5.6.1",
		"graphql-request": "^7.1.0",
		"graphql-yoga": "^5.4.0",
		"hast-util-from-html-isomorphic": "^2.0.0",
		"js-cookie": "^3.0.5",
		"next": "15.0.2",
		"next-auth": "^5.0.0-beta.25",
		"next-cache-toolbar": "^0.2.1",
		"next-intl": "^3.22.0",
		"next-mdx-remote": "^5.0.0",
		"openai": "^4.68.4",
		"react": "19.0.0-rc-65a56d0e-20241020",
		"react-dom": "19.0.0-rc-65a56d0e-20241020",
		"react-hotkeys-hook": "^4.5.0",
		"react-string-replace": "^1.1.1",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"remark-flexible-toc": "^1.1.1",
		"remark-gfm": "^4.0.0",
		"swr": "^2.2.5",
		"tsparticles": "^3.5.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@prisma/nextjs-monorepo-workaround-plugin": "^5.16.2",
		"@types/canvas-confetti": "^1.6.4",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^20",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"axios": "^1.7.5",
		"cheerio": "^1.0.0",
		"daisyui": "^4.12.10",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"npm-run-all": "^4.1.5",
		"postcss": "^8",
		"prisma": "^5.16.1",
		"tailwindcss": "^3.4.1",
		"tsx": "^4.19.0",
		"typescript": "^5"
	},
	"cspell": {
		"version": "0.2",
		"ignorePaths": [],
		"dictionaryDefinitions": [],
		"dictionaries": [],
		"words": [
			"ahooks",
			"biomejs",
			"clsx",
			"daisyui",
			"emerald",
			"Frontmatter",
			"myeongjo",
			"Nanum",
			"NEXTAUTH",
			"Noto",
			"openai",
			"Signin",
			"signout",
			"summerscar",
			"svgr",
			"tailwindcss",
			"Topik"
		],
		"ignoreWords": [],
		"import": []
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --files-ignore-unknown=true",
			"biome check --write --no-errors-on-unmatched",
			"biome check --write --organize-imports-enabled=false --no-errors-on-unmatched",
			"biome check --write --unsafe --no-errors-on-unmatched",
			"biome format --write --no-errors-on-unmatched",
			"biome lint --write --no-errors-on-unmatched"
		],
		"*": [
			"biome check --no-errors-on-unmatched --files-ignore-unknown=true"
		],
		"./mdx/**/*.{mdx,md}": [
			"npm run script:docs-modified"
		]
	},
	"volta": {
		"node": "20.17.0"
	},
	"overrides": {
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"react": "19.0.0-rc-65a56d0e-20241020",
		"react-dom": "19.0.0-rc-65a56d0e-20241020",
		"next": "15.0.2"
	}
}
