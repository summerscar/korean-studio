{
	"name": "korean-studio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel keystone:dev next:dev",
		"build": "npm run keystone:build && npm run next:build",
		"postinstall": "keystone postinstall",
		"keystone:dev": "keystone dev",
		"keystone:build": "keystone build --no-ui",
		"keystone:start": "keystone start",
		"next:dev": "next dev",
		"next:build": "next build",
		"next:start": "next start",
		"lint": "biome lint -- apply <files>",
		"format": "biome format <files> --write",
		"check": "biome check --apply <files>",
		"prepare": "husky",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"@keystone-6/auth": "^8.0.0",
		"@keystone-6/core": "^6.2.0",
		"@keystone-6/document-renderer": "^1.1.2",
		"@keystone-6/fields-document": "^9.1.0",
		"@mdx-js/loader": "^3.0.1",
		"@mdx-js/react": "^3.0.1",
		"@next/mdx": "^14.2.5",
		"@next/third-parties": "^14.2.5",
		"@prisma/client": "^5.16.1",
		"@svgr/webpack": "^8.1.0",
		"@types/mdx": "^2.0.13",
		"@vercel/analytics": "^1.3.1",
		"@vercel/speed-insights": "^1.0.12",
		"ahooks": "^3.8.0",
		"canvas-confetti": "^1.9.3",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"es-hangul": "^1.3.5",
		"github-markdown-css": "^5.6.1",
		"graphql-request": "^7.1.0",
		"graphql-yoga": "^5.4.0",
		"js-cookie": "^3.0.5",
		"next": "14.3.0-canary.33",
		"next-auth": "^5.0.0-beta.19",
		"next-intl": "^3.15.0",
		"next-mdx-remote": "^5.0.0",
		"react": "^18",
		"react-dom": "^18",
		"react-hotkeys-hook": "^4.5.0",
		"react-string-replace": "^1.1.1",
		"swr": "^2.2.5",
		"tsparticles": "^3.5.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.1",
		"@prisma/nextjs-monorepo-workaround-plugin": "^5.16.2",
		"@types/canvas-confetti": "^1.6.4",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"daisyui": "^4.12.10",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"npm-run-all": "^4.1.5",
		"postcss": "^8",
		"prisma": "^5.16.1",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	},
	"overrides": {
		"@next/third-parties": {
			"next": "14.3.0-canary.33"
		},
		"@vercel/analytics": {
			"next": "14.3.0-canary.33"
		},
		"@vercel/speed-insights": {
			"next": "14.3.0-canary.33"
		},
		"next-auth": {
			"next": "14.3.0-canary.33"
		},
		"next-intl": {
			"next": "14.3.0-canary.33"
		}
	},
	"cspell": {
		"version": "0.2",
		"ignorePaths": [],
		"dictionaryDefinitions": [],
		"dictionaries": [],
		"words": [
			"ahooks",
			"biomejs",
			"clsx",
			"daisyui",
			"Frontmatter",
			"myeongjo",
			"Nanum",
			"NEXTAUTH",
			"nord",
			"Noto",
			"Signin",
			"signout",
			"summerscar",
			"svgr",
			"tailwindcss",
			"Topik"
		],
		"ignoreWords": [],
		"import": []
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --files-ignore-unknown=true",
			"biome check --write --no-errors-on-unmatched",
			"biome check --write --organize-imports-enabled=false --no-errors-on-unmatched",
			"biome check --write --unsafe --no-errors-on-unmatched",
			"biome format --write --no-errors-on-unmatched",
			"biome lint --write --no-errors-on-unmatched"
		],
		"*": [
			"biome check --no-errors-on-unmatched --files-ignore-unknown=true"
		]
	}
}
